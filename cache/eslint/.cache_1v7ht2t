nimationProps> = (props) => {\n    const [tween, setTween] = useState<gsap.core.Tween>();\n    const ref = useRef() as MutableRefObject<HTMLDivElement>;\n    const options = { ...defaultOption, ...props.options };\n\n    const generateChildren = useCallback((): React.ReactElement => {\n        return React.createElement('div', { dangerouslySetInnerHTML: { __html: props.children } });\n    }, [props.children]);\n\n    const children = typeof props.children === 'string' ? generateChildren() : props.children;\n\n    useEffect(() => {\n        gsap.set(ref.current.children, { y: options.initialVisibility ? 0 : options.target === 'bottom' ? '100%' : '-100%' });\n    }, []);\n\n    useEffect(() => {\n        setTween(\n            gsap.to(ref.current.children, {\n                y: props.mode !== 'reverse' ? 0 : '100%',\n                ease: options.easing,\n                duration: options.duration,\n                delay: options.delay,\n                paused: true\n            })\n        );\n    }, [props.options, props.mode]);\n\n    useEffect(() => {\n        if (['play', 'reverse'].includes(props.mode ?? 'auto')) tween?.play();\n    }, [tween, props.mode]);\n\n    useEffect(() => {\n        if (!options.intersection || !tween) return;\n\n        const observer = new IntersectionObserver(\n            (entries) => {\n                entries.forEach(({ intersectionRatio }) => {\n                    if (intersectionRatio > options.animateThreshold) {\n                        observer.disconnect();\n                        tween.play();\n                    }\n                });\n            },\n            {\n                rootMargin: options.rootMargin,\n                threshold: options.threshold\n            }\n        );\n\n        observer.observe(ref.current);\n\n        return () => observer.disconnect();\n    }, [options.intersection, tween]);\n\n    return (\n        <Container ref={ref} className={props.className}>\n            {children}\n        </Container>\n    );\n};\n\nexport default AppearAnimation;\n","D:\\projects\\only-template-next\\src\\components\\common\\Footer\\Footer.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Footer\\Footer.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Header\\Header.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Header\\Header.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Image\\Image.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Image\\Image.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Layout\\Layout.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\Layout\\Layout.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\LinkConditional\\LinkConditional.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\LinkConditional\\LinkConditional.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\ParallaxElement\\ParallaxElement.styled.tsx",[],[],"D:\\projects\\only-template-next\\src\\components\\common\\ParallaxElement\\ParallaxElement.tsx",["254"],[],"import React, { MutableRefObject, useEffect, useRef } from 'react';\nimport { Container } from './ParallaxElement.styled';\nimport { useParallax } from '@/components/common/ParallaxWrapper/ParallaxWrapper';\nimport { BaseUI } from '@/interfaces';\nimport TweenVars = gsap.TweenVars;\nimport useIsMobile from '@/hooks/useIsMobile';\n\nexport interface ParallaxElementProps extends BaseUI {\n    options?: ParallaxOptions;\n}\n\nexport type ParallaxOptions = {\n    from?: TweenVars;\n    to?: TweenVars;\n};\n\nconst defaultOptions: Required<ParallaxOptions> = {\n    from: {},\n    to: {}\n};\n\nconst ParallaxElement: React.FC<ParallaxElementProps> = (props) => {\n    const parallax = useParallax();\n    const ref = useRef() as MutableRefObject<HTMLDivElement>;\n    const isMobile = useIs